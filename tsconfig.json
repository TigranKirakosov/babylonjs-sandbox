{
    "compilerOptions": {
        "moduleResolution": "node",
        "outDir": "./dist/",
        "sourceMap": true,
        "noImplicitAny": false,
        "module": "es6",
        "target": "es5",
        "allowJs": true,
        "allowSyntheticDefaultImports": true,
        "esModuleInterop" : true,
        "baseUrl": "./",
        "resolveJsonModule": true
    },
    "exclude": ["node_modules"]
}
